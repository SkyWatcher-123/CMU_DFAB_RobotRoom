<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="abb_irb6640_185_280" params="prefix:= ''
  add_eef:= ${add_eef_xacro}
  eef_filename:= ${eef_filename_xacro}
  tcp_offset_xyz:= ${tcp_offset_xyz_xacro}
  tcp_offset_rpy:= ${tcp_offset_rpy_xacro}
  add_sensor_mount:= ${add_sensor_mount_xacro}
  sensor_mount_filename:= ${sensor_mount_filename_xacro}
  sensor_mount_offset_xyz:= ${sensor_mount_offset_xyz_xacro}
  sensor_mount_offset_rpy:= ${sensor_mount_offset_rpy_xacro}
  ">
  <!-- Track link list -->
  <!-- <link name="${prefix}world"/> -->
  <!-- Track link -->
  <link name="${prefix}track">
    <collision name="collision">
      <geometry>
        <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/collision/abb_irb6640_track.stl"/>
      </geometry>
    </collision>
    <visual name="visual">
      <geometry>
        <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/visual/abb_irb6640_track.stl"/>
      </geometry>
      <material name="yellow">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
  </link>
  <!-- track loader link-->
  <link name="${prefix}track_loader">
    <collision name="collision">
      <geometry>
        <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/collision/abb_irb6640_link1_track_loader.stl"/>
      </geometry>
    </collision>
    <visual name="visual">
      <geometry>
        <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/visual/abb_irb6640_link1_track_loader.stl"/>
      </geometry>
      <material name="yellow">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
    </link>
    <!-- IRB6640 link list -->
    <link name="${prefix}base_link">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/collision/base_link.stl"/>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/visual/base_link.dae"/>
        </geometry>
        <material name="yellow">
          <color rgba="1 1 0 1"/>
        </material>
      </visual>
    </link>
    <link name="${prefix}link_1">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/collision/link_1.stl"/>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/visual/link_1.dae"/>
        </geometry>
        <material name="yellow"/>
      </visual>
    </link>
    <link name="${prefix}link_2">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/collision/link_2.stl"/>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/visual/link_2.dae"/>
        </geometry>
        <material name="yellow"/>
      </visual>
    </link>
    <link name="${prefix}link_3">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/collision/link_3.stl"/>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/visual/link_3.dae"/>
        </geometry>
        <material name="yellow"/>
      </visual>
    </link>
    <link name="${prefix}link_4">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/collision/link_4.stl"/>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/visual/link_4.dae"/>
        </geometry>
        <material name="yellow"/>
      </visual>
    </link>
    <link name="${prefix}link_5">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/collision/link_5.stl"/>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/visual/link_5.dae"/>
        </geometry>
        <material name="yellow"/>
      </visual>
    </link>
    <link name="${prefix}link_6">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/collision/link_6.stl"/>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/visual/link_6.dae"/>
        </geometry>
        <material name="yellow"/>
      </visual>
    </link>


    

    <!--Cylinder and piston -->
    <link name="${prefix}link_cylinder">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/collision/cylinder_link.stl"/>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/visual/cylinder_link.dae"/>
        </geometry>
        <material name="yellow"/>
      </visual>
    </link>
    <link name="${prefix}link_piston">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/collision/piston_link.stl"/>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/visual/piston_link.dae"/>
        </geometry>
        <material name="yellow"/>
      </visual>
    </link>
    <!-- end of link list -->
    <!-- Track joint list -->

    <!-- end of track joint list -->

    <!-- IRB6640 joint list -->
    <joint type="revolute" name="${prefix}joint_1">
      <origin xyz="0 0 0.780" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1"/>
      <limit effort="0" lower="-2.967" upper="2.967" velocity="1.7453"/>
    </joint>
    <joint type="revolute" name="${prefix}joint_2">
      <origin xyz="0.320 0 0" rpy="0 0 0"/>
      <axis xyz="0 1 0"/>
      <parent link="${prefix}link_1"/>
      <child link="${prefix}link_2"/>
      <limit effort="0" lower="-1.134" upper="1.4855" velocity="1.5707"/>
    </joint>
    <joint type="revolute" name="${prefix}joint_3">
      <origin xyz="0 0 1.075" rpy="0 0 0"/>
      <axis xyz="0 1 0"/>
      <parent link="${prefix}link_2"/>
      <child link="${prefix}link_3"/>
      <limit effort="0" lower="-3.142" upper="1.222" velocity="1.5707"/>
    </joint>
    <joint type="revolute" name="${prefix}joint_4">
      <origin xyz="0 0 0.2" rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <parent link="${prefix}link_3"/>
      <child link="${prefix}link_4"/>
      <limit effort="0" lower="-5.236" upper="5.236" velocity="2.9671"/>
    </joint>
    <joint type="revolute" name="${prefix}joint_5">
      <origin xyz="1.392 0 0 " rpy="0 0 0"/>
      <axis xyz="0 1 0"/>
      <parent link="${prefix}link_4"/>
      <child link="${prefix}link_5"/>
      <limit effort="0" lower="-2.094" upper="2.094" velocity="2.4435"/>
    </joint>
    <joint type="revolute" name="${prefix}joint_6">
      <origin xyz="0.2 0 0 " rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <parent link="${prefix}link_5"/>
      <child link="${prefix}link_6"/>
      <limit effort="0" lower="-6.283" upper="6.283" velocity="3.3161"/>
    </joint>
    <!-- <joint type="fixed" name="${prefix}joint_6-tool0">
      <parent link="${prefix}link_6"/>
      <child link="${prefix}tool0"/>
      <origin xyz="0 0 0" rpy="0 1.57079632679 0"/>
    </joint> -->
    <joint type="continuous" name="${prefix}joint_cylinder">
      <origin xyz="-0.3647 0 -0.1455" rpy="0 0 0"/>
      <axis xyz="0 1 0"/>
      <parent link="${prefix}link_1"/>
      <child link="${prefix}link_cylinder"/>
      <mimic joint="${prefix}joint_2" multiplier="-0.25"/>
    </joint>
    <joint type="continuous" name="${prefix}joint_piston">
      <origin xyz="-0.22 0 -0.0672" rpy="0 0 0"/>
      <axis xyz="0 1 0"/>
      <parent link="${prefix}link_2"/>
      <child link="${prefix}link_piston"/>
      <mimic joint="${prefix}joint_2" multiplier="-1.25"/>
    </joint>
    <!-- end of IRB6640 joint list -->

    <!-- ROS base_link to ABB World Coordinates transform -->
    <!-- <joint name="${prefix}base_track" type="fixed">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <parent link="${prefix}world"/>
      <child link="${prefix}track"/>
    </joint> -->

  <joint name="${prefix}track1" type="prismatic">
    <parent link="${prefix}track"/>
    <child link="${prefix}track_loader"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/> <!-- Movement along the x-axis -->
    <limit lower="0.0" upper="5.69" effort="100" velocity="1.0"/>
  </joint>

  <joint name="${prefix}loader_base" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="${prefix}track_loader"/>
    <child link="${prefix}base_link"/>
  </joint>


    <!-- due to the setup of the end-effector plate, it is rotated 45 degrees around the z axis -->

    <link name="${prefix}tool0_rt1"/>
      <joint type="fixed" name="${prefix}joint_tool0_rotate1">
        <parent link="${prefix}link_6"/>
        <child link="${prefix}tool0_rt1"/>
        <origin xyz="0 0 0" rpy="0 ${pi/2} 0"/>
    </joint>

    


      <!-- add optional end effector -->

    <xacro:if value="${add_eef}">

      <link name="${prefix}tool0_mount">
      <visual>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <geometry>
            <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/endEffector/${eef_filename}"/>
          </geometry>
        </visual>
        <collision>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <geometry>
            <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/endEffector/${eef_filename}"/>
          </geometry>
        </collision>
      </link>

        <joint type="fixed" name="${prefix}joint_tool0_rotate2">
          <parent link="${prefix}tool0_rt1"/>
          <child link="${prefix}tool0_mount"/>
          <origin xyz="0 0 0" rpy="0 0 ${pi*4/6}"/>
      </joint>

      <link name="${prefix}tool0">
      </link>

    
      <joint type="fixed" name="${prefix}joint_6-tool0">
        <parent link="${prefix}tool0_mount"/>
        <child link="${prefix}tool0"/>
        <origin xyz="${tcp_offset_xyz}" rpy="${tcp_offset_rpy}"/>
      </joint>
    </xacro:if>

    <xacro:unless value="${add_eef}">

      <link name="${prefix}tool0_mount"/>
      <joint type="fixed" name="${prefix}joint_tool0_rotate2">
        <parent link="${prefix}tool0_rt1"/>
        <child link="${prefix}tool0_mount"/>
        <origin xyz="0 0 0" rpy="0 0 ${pi/6}"/>
      </joint>
      <link name="${prefix}tool0"/>
      <joint type="fixed" name="${prefix}joint_6-tool0">
        <parent link="${prefix}tool0_mount"/>
        <child link="${prefix}tool0"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
      </joint>
      
    </xacro:unless>

    <!-- add optional sensor_mount -->

    <xacro:if value="${add_sensor_mount}">
      <link name="${prefix}sensor_mount">
        <visual>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <geometry>
            <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/sensor/${sensor_mount_filename}"/>
          </geometry>
        </visual>
        <collision>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <geometry>
            <mesh filename="package://abb_irb6640_support/meshes/irb6640_185_280/sensor/${sensor_mount_filename}"/>
          </geometry>
        </collision>
      </link>
      <joint type="fixed" name="${prefix}joint_sensor_mount">
        <parent link="${prefix}tool0_mount"/>
        <child link="${prefix}sensor_mount"/>
        <origin xyz="${sensor_mount_offset_xyz}" rpy="${sensor_mount_offset_rpy}"/>
      </joint>
    </xacro:if>

    <xacro:unless value="${add_sensor_mount}">
      <link name="${prefix}sensor_mount"/>
      <joint type="fixed" name="${prefix}joint_sensor_mount">
        <parent link="${prefix}tool0"/>
        <child link="${prefix}sensor_mount"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
      </joint>
    </xacro:unless>
  </xacro:macro>
</robot>
